{
  "expo": {
    "scheme": "rms",
    "name": "Remote-MediaServer-Frontend",
    "slug": "Remote-MediaServer-Frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0a0c1a",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0c1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0c1a",
        "userInterfaceStyle": "dark"
      },
      "package": "com.owenray.RemoteMediaServer"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "backgroundColor": "#0a0c1a",
      "userInterfaceStyle": "dark"
    },
    "extra": {
      "eas": {
        "projectId": "4ac9a301-bf2c-4a32-abb8-e806d0bca1b4"
      }
    }
  }
}
